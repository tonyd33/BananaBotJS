{
  "name": "BananaBotJS",
  "version": "1.0.0",
  "description": "BananaBot in JavaScript",
  "main": "build/index.js",
  "scripts": {
    "prettier-format": "prettier --config .prettierrc.json 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "start": "nodemon --exec DATABASE=DEVELOPMENT BOT_TOKEN=`cat bot_token` node --loader ts-node/esm/transpile-only src/index.ts",
    "serve": "DATABASE=PRODUCTION BOT_TOKEN=`cat bot_token` node build/index.js",
    "test": "DATABASE=TESTING jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonyd33/BananaBotJS.git"
  },
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "bot",
    "discord",
    "typescript"
  ],
  "author": "Tony Du <tonydu121@hotmail.com> [https://github.com/tonyd33]",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tonyd33/BananaBotJS/issues"
  },
  "homepage": "https://github.com/oceanroleplay/BananaBotJS#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-jest": "^27.4.2",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.3",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@discordjs/voice": "^0.7.5",
    "@discordx/importer": "^1.1.2",
    "@discordx/music": "^3.0.16",
    "@discordx/utilities": "^3.1.5",
    "discord.js": "^13.3.1",
    "discordx": "^9.0.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.12.0-beta.1",
    "sqlite3": "^5.0.2",
    "yargs": "^16.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
